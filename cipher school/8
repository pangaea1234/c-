#include<bits/stdc++.h>
using namespace std;

int pivotIndex(vector<int>& nums) {
    int s = 0;
    for(int i = 0;i<nums.size();i++)s+=nums[i];

    for(int i = 0;i<nums.size();i++){
        if(i == 0 && s - nums[i] == 0){
            return i;
        }else if(s-nums[i]-nums[i-1] == nums[i-1])return i;
        if(i>0)nums[i]+=nums[i-1];
    }

    return -1;
}

int main(){

    return 0;
}
