#include<bits/stdc++.h>

using namespace std;

int findInd(int*arr,int num){ //int arr[]
    int size = sizeof(arr)/sizeof(arr[0]);
    int ans = -1;
    for(int i =0;i<size;i++){
        if(arr[i] == num){
            ans = i;
            break;
        }
    }
    return ans;
}

int findLstInd(int*arr,int num){
    int size = sizeof(arr)/sizeof(arr[0]);
    int ans = 0;
    for(int i = 0;i<size;i++){
        if(arr[i] == num){
            ans = i;
        }
    }

    return ans;
}

