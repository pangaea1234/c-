#include<bits/stdc++.h>

using namespace std;
void rotateArr(vector<int>&v,int r){
    reverse(v.begin(),v.end());
    reverse(v.begin(),v.begin()+r);
    reverse(v.begin()+r,v.end());
    for(int i = 0;i<v.size();i++)cout<<v[i]<<" ";
    cout<<endl;
}

int main(){

    vector<int>v = {4,3,2,7,8,2};
    rotateArr(v,2);
    return 0;
}
